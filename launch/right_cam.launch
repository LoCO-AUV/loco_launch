<launch>
  <group ns="/loco_cams/right">
    <node pkg="libuvc_camera" type="camera_node" name="loco_right_cam" respawn="true" respawn_delay="10">
      <param name="vendor" value="0x05a3"/>
      <param name="product" value="0x9422"/>
      <param name="width" type="int" value="640" />
      <param name="height" type="int" value="480" />
    </node>
  </group>
</launch>
